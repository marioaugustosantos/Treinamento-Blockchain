      <md-card-content>\n        <div class=\"md-layout\">\n\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n            <md-field :class=\"getValidationClass('id')\">\n              <label>Número do Processo (ID)</label>\n              <md-input v-model=\"processo.id\" id=\"id\" type=\"text\"></md-input>\n              <span class=\"md-error\" v-if=\"!$v.processo.id.required\">Informe o número do processo para realizar a pesquisa</span>\n            </md-field>\n          </div>\n\n           <div class=\"md-layout-item md-size-100 text-right\">\n            <md-button class=\"md-raised md-success\" type =\"submit\" @click.prevent=\"PesquisarProcesso\"\n              >Pesquisar Processo</md-button>\n          </div>\n\n\n          <!--<div class=\"md-layout-item md-small-size-100 md-size-33\">\n            <md-field>\n              <label>Company (disabled)</label>\n              <md-input v-model=\"disabled\" disabled></md-input>\n            </md-field>\n          </div> -->\n          \n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n            <md-field>\n              <label>Nome Comercial do Medicamento</label>\n              <md-input v-model=\"processo.nomeComercialMedicamento\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field>\n              <label>Número RENAME</label>\n              <md-input v-model=\"processo.numRename\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field>\n              <label>Status Atual</label>\n              <md-input v-model=\"processo.status\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-100\">\n            <md-field>\n              <label>Organização Atual</label>\n              <md-input v-model=\"processo.instancia\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-100\">\n            <md-field>\n              <label>Organização Movimentação</label>\n              <md-input v-model=\"orgMovimentacao\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n\n          <div class=\"md-layout-item md-small-size-100 md-size-100\">\n            <md-field>\n              <label>Operador</label>\n               <md-input v-model=\"processo.operador\" type=\"text\" disabled ></md-input>\n            </md-field>\n          </div>\n\n          <div class=\"md-layout md-gutter\">\n            <div class=\"md-layout-item md-small-size-100\">\n              <!--<md-field :class=\"getValidationClass('statusProcesso')\"> -->\n                <md-field> \n                <label for=\"statusProcesso\">Selecione o novo Status do Processo</label>\n                <md-select name=\"statusProcesso\" id=\"statusProcesso\" v-model=\"processo.status\" md-dense>\n                  <md-option></md-option>\n                  <md-option value=\"Solicitado_SMS\">Solicitado na Secretaria Municipal de Saude</md-option>\n                  <md-option value=\"Solicitado_Almoxarifado\">Solicitado no Almoxarifado</md-option>\n                  <md-option value=\"Solicitado_Compra\">Solicitado para Compra</md-option>\n                  <md-option value=\"Disponivel_Almoxarifado\">Disponivel no Almoxarifado</md-option>\n                  <md-option value=\"Disponivel_Retirada\">Disponivel para Retirada</md-option>\n                  <md-option value=\"Concluido_Entregue_Paciente\">Entregue ao Paciente</md-option>\n                </md-select>\n                  <!--<span class=\"md-error\" v-if=\"!$v.processo.status.required\">Informe o novo status do processo!</span>-->\n              </md-field>\n            </div>\n          </div>\n         \n          <div class=\"md-layout-item md-size-100 text-right\">\n            <md-button class=\"md-raised md-success\" type =\"submit\" @click.prevent=\"MovimentarProcesso\" \n              >Movimentar Processo</md-button\n            >\n          </div>\n         \n        </div>\n      </md-card-content>\n    </md-card>\n  </form>\n\n</template>\n\n<script>\n\n\nimport { validationMixin } from 'vuelidate';\n\nimport {\n    required,   \n    minLength\n   \n  } from \"vuelidate/lib/validators\";\n\nexport default {\n\n  name: \"edit-profile-form\",\n  mixins: [validationMixin],\n  props: {\n    dataBackgroundColor: {\n      type: String,\n      default: \"\",\n    },\n    tableHeaderColor: {\n      type: String,\n      default: \"\"\n    },\n  },\n\n  data() {\n    return {\n\n      processo:{\n\n      id: null,\n      instancia: null,\n      nomeComercialMedicamento: null,\n      numRename: null,\n      status: null,\n      operador: null\n\n      },\n\n      orgMovimentacao: \"SMS\",\n\n      processos: [],\n\n      type: [\"\", \"info\", \"success\", \"warning\", \"danger\"],\n      notifications: {\n        topCenter: false\n      },\n\n      statusProcesso: null,\n         \n    };\n  },\n\n  validations: {\n      processo: {\n         id: {\n           required,\n           minLength: minLength(3)\n        },\n         /*status: {\n           required       \n        }  */             \n\n      }\n      },\n\n\n methods:{\n\n   getValidationClass (fieldName) {\n        const field = this.$v.processo[fieldName]\n\n        if (field) {\n          return {\n            'md-invalid': field.$invalid && field.$dirty\n          }\n        }\n      },\n         async PesquisarProcesso(){\n\n            this.$v.$touch();\n\n            if (!this.$v.$invalid) {\n                   \n              try {\n             \n               const response = await this.$http.get('http://localhost:8080/api/PesquisarProcessoPorNumero/'+this.processo.id)  ; \n              //.then( res => {\n                 console.log(response.data);\n                 \n                 const retorno = response.data;\n\n                 var convert = JSON.parse(retorno.response);\n\n                  console.log(convert);\n\n                  this.processo.nomeComercialMedicamento = convert.NomeComercialMedicamento;\n                  this.processo.instancia = convert.Instancia;\n                  this.processo.numRename = convert.NumRename;\n                  this.processo.status = convert.Status;\n                  this.processo.operador = convert.Operador;\n\n                  this.processos[0] = this.processo;           \n              \n           } catch(e) {\n              console.error(e);\n          }\n            }\n                 \n        },\n\n       async MovimentarProcesso(){\n\n         //this.$v.$touch();\n\n          //if (!this.$v.$invalid) {\n\n           this.processo.instancia = this.orgMovimentacao;\n           \n            await this.$http.put('http://localhost:8080/api/MovimentarProcesso/',this.processo)    \n                //.then((res) => {\n                  .then(() => {\n                     this.salvoSucessoNotify('top', 'center');\n                })\n                //.catch((error) => {\n                  .catch(() => {\n                     this.erroSalvarNotify('top', 'center');\n                 }).finally(() => {\n                     //Perform action in always\n                 });\n            //}\n        },\n\n     \n    salvoSucessoNotify(verticalAlign, horizontalAlign) {\n      //var color = Math.floor(Math.random() * 4 + 1);\n      this.$notify({\n        message:\n          \"Movimentação do Processo Judicial salvo com sucesso na blockchain.\",\n        icon: \"add_alert\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: \"success\"\n      });\n    },\n\n    erroSalvarNotify(verticalAlign, horizontalAlign) {\n      //var color = Math.floor(Math.random() * 4 + 1);\n      this.$notify({\n        message:\n          \"ATENÇÃO: O Processo não pôde ser salvo na Blockchain!.\",\n        icon: \"add_alert\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: \"danger\"\n      });\n    },\n\n          \n    },\n\n\n};\n</script>\n<style></style>\n"]}]}