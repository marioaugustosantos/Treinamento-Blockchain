out-item md-small-size-100 md-size-33\">\n            <md-field>\n              <label>Organização</label>\n              <md-input v-model=\"form.instancia\" type=\"text\" disabled ></md-input>\n            </md-field>\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field>\n              <label>Operador</label>\n              <md-input v-model=\"form.operador\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n            \n            <md-field :class=\"getValidationClass('id')\">            \n              <label>Número do Processo (ID)</label>\n              <md-input v-model=\"form.id\" id=\"id\" type=\"text\" :disabled=\"sending\"></md-input>\n              <span class=\"md-error\" v-if=\"!$v.form.id.required\">O número do processo é obrigatório!</span>\n              <!--<span class=\"md-error\" v-else-if=\"!$v.form.id.minlength\">Invalid first name</span>-->\n            </md-field>\n\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n\n          <md-field :class=\"getValidationClass('nomeComercialMedicamento')\">\n            \n             <label>Nome Comercial do Medicamento</label>\n             <md-input v-model=\"form.nomeComercialMedicamento\" id=\"nomeComercialMedicamento\" type=\"text\" :disabled=\"sending\"></md-input>\n             <span class=\"md-error\" v-if=\"!$v.form.nomeComercialMedicamento.required\">O nome comercial do medicamento é obrigatório!</span>           \n          </md-field>\n\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field :class=\"getValidationClass('numRename')\">\n              <label>Número RENAME</label>\n              <md-input v-model=\"form.numRename\" id=\"numRename\" type=\"text\" :disabled=\"sending\"></md-input>\n              <span class=\"md-error\" v-if=\"!$v.form.numRename.required\">O numero Rename do medicamento é obrigatório!</span>           \n            </md-field>\n          </div>\n                      \n          <div class=\"md-layout-item md-size-100 text-right\">\n            <md-button class=\"md-raised md-success\" type =\"submit\"\n              >Cadastrar Processo</md-button\n            >\n          </div>\n        </div>\n        \n\n      </md-card-content>\n    </md-card>\n  </form>\n</template>\n\n<script>\n\nimport axios from 'axios';\nimport { validationMixin } from 'vuelidate';\n\n  import {\n    required,   \n    minLength\n   \n  } from \"vuelidate/lib/validators\";\n\nexport default {\n  name: \"edit-profile-form\",\n  mixins: [validationMixin],\n  props: {\n    dataBackgroundColor: {\n      type: String,\n      default: \"\",\n    },\n  },\n  data() {\n    return {\n\n      form:{\n      id: null,\n      instancia: 'MPBA',\n      nomeComercialMedicamento: null,\n      numRename: null,\n      status: 'Criado_MP',\n      operador: 'Mário Augusto Santos'\n\n      },\n\n      type: [\"\", \"info\", \"success\", \"warning\", \"danger\"],\n      notifications: {\n        topCenter: false\n      },\n\n      sending: false,\n      showProgressBar: false\n\n      \n         \n    };\n  },\n\n  validations: {\n      form: {\n         id: {\n           required,\n           minLength: minLength(3)\n        },\n          nomeComercialMedicamento: {\n           required,        \n        },\n          numRename: {\n           required,       \n        }\n\n      }\n      },\n\n methods:{\n\ngetValidationClass (fieldName) {\n        const field = this.$v.form[fieldName]\n\n        if (field) {\n          return {\n            'md-invalid': field.$invalid && field.$dirty\n          }\n        }\n      },\n\n\n    submitForm(){\n\n      this.saveUser();\n  \n      this.$v.$touch();\n\n\n      if (!this.$v.$invalid) {\n\n    \n            axios.post('http://localhost:8080/api/CadastrarProcessoJudicial/', this.form)\n                //.then((res) => {\n                 .then(() => {\n                     this.salvoSucessoNotify('top', 'center');\n                 })\n                .catch((error) => {\n\n                   var str = JSON.stringify(error.response.data.error.responses);\n                   var parsed = JSON.parse(str);\n                   var erro = parsed[0]; \n                     \n                   //alert (JSON.stringify(erro.response.message));\n                   var mensagemErro = erro.response.message;\n\n                 this.erroSalvarNotify('top', 'center', mensagemErro);\n\n                 }).finally(() => {\n                     //Perform action in always\n                 });\n    }\n\n    this.sending = false;\n \n\n\n        },\n\n        saveUser () {\n\n        //this.showProgressBar = true;\n        this.sending = true\n\n        // Instead of this timeout, here you can call your API\n        //window.setTimeout(() => {\n         // this.lastUser = `${this.form.firstName} ${this.form.lastName}`\n          //this.userSaved = true\n          //this.sending = false\n        //  this.showProgressBar = false\n          //this.clearForm()\n       // }, 6000)\n      },\n\n    salvoSucessoNotify(verticalAlign, horizontalAlign) {\n\n      this.sending = false;\n      \n      this.$notify({\n        message:\n          \"Processo Judicial salvo com sucesso na blockchain.\",\n        icon: \"add_alert\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: \"success\"\n      });\n    },\n\n    erroSalvarNotify(verticalAlign, horizontalAlign, mensagemErro) {\n      \n      this.$notify({\n        message:\n          mensagemErro,\n        icon: \"add_alert\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: \"danger\"\n      });\n    },\n        \n    },\n\n\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .md-progress-bar {\n    position: absolute;\