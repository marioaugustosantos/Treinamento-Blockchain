AACA,KAVA;AAYA,IAAA,UAZA,wBAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,KAAA,CAAA,EAAA,CAAA,MAAA;;AAFA,oBAIA,KAAA,CAAA,EAAA,CAAA,QAJA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAQA,KAAA,CAAA,KAAA,CAAA,GAAA,CAAA,0DAAA,KAAA,CAAA,QAAA,CAAA,EAAA,CARA;;AAAA;AAQA,gBAAA,QARA;AASA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AAEA,gBAAA,OAZA,GAYA,QAAA,CAAA,IAZA;AAcA,gBAAA,OAdA,GAcA,IAAA,CAAA,KAAA,CAAA,OAAA,CAAA,QAAA,CAdA;AAgBA,gBAAA,KAAA,CAAA,QAAA,CAAA,wBAAA,GAAA,OAAA,CAAA,wBAAA;AACA,gBAAA,KAAA,CAAA,QAAA,CAAA,SAAA,GAAA,OAAA,CAAA,SAAA;AACA,gBAAA,KAAA,CAAA,QAAA,CAAA,SAAA,GAAA,OAAA,CAAA,SAAA;AACA,gBAAA,KAAA,CAAA,QAAA,CAAA,MAAA,GAAA,OAAA,CAAA,MAAA;AACA,gBAAA,KAAA,CAAA,QAAA,CAAA,QAAA,GAAA,OAAA,CAAA,QAAA;AAEA,gBAAA,KAAA,CAAA,SAAA,CAAA,CAAA,IAAA,KAAA,CAAA,QAAA,CAtBA,CAwBA;AACA;;AAzBA;AAAA;;AAAA;AAAA;AAAA;AA6BA,gBAAA,OAAA,CAAA,KAAA;;AA7BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiCA,KA7CA;AAgDA,IAAA,SAhDA,qBAgDA,aAhDA,EAgDA,eAhDA,EAgDA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,KAAA,CAAA,GAAA,CAAA,CAAA;AACA,WAAA,OAAA,CAAA;AACA,QAAA,OAAA,EACA,oDAFA;AAGA,QAAA,IAAA,EAAA,WAHA;AAIA,QAAA,eAAA,EAAA,eAJA;AAKA,QAAA,aAAA,EAAA,aALA;AAMA,QAAA,IAAA,EAAA,KAAA,IAAA,CAAA,KAAA;AANA,OAAA;AAQA;AA1DA;AAlDA,CAAA","sourcesContent":["<template>\n  <form method=\"GET\" @submit.prevent=\"submitForm\">\n    <md-card>\n      <md-card-header :data-background-color=\"dataBackgroundColor\">\n        <h4 class=\"title\">Processo Judicial</h4>\n        <p class=\"category\">Acompanhar</p>\n      </md-card-header>\n\n      <md-card-content>\n        <div class=\"md-layout\">\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n            <md-field :class=\"getValidationClass('ID')\">\n              <label>Número do Processo (ID)</label>\n               <md-input v-model=\"processo.ID\"  id=\"id\" type=\"text\"></md-input>\n            <span class=\"md-error\" v-if=\"!$v.processo.ID.required\">Informe o número do processo!</span>\n            </md-field>\n\n            <md-button class=\"md-raised md-success\" type =\"submit\" \n              >Pesquisar</md-button\n            >\n          </div>\n           \n        \n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n            <md-field>\n              <label>Nome Comercial do Medicamento</label>\n               <md-input v-model=\"processo.NomeComercialMedicamento\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n\n        \n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field>\n              <label>Número RENAME</label>\n               <md-input v-model=\"processo.NumRename\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field>\n              <label>STATUS</label>\n              <md-input v-model=\"processo.Status\" type=\"text\" disabled></md-input>\n            </md-field>\n          </div>\n          <div class=\"md-layout-item md-small-size-100 md-size-100\">\n            <md-field>\n              <label>Organização</label>\n               <md-input v-model=\"processo.Instancia\" type=\"text\" disabled ></md-input>\n            </md-field>\n          </div>\n\n          <div class=\"md-layout-item md-small-size-100 md-size-100\">\n            <md-field>\n              <label>Operador</label>\n               <md-input v-model=\"processo.Operador\" type=\"text\" disabled ></md-input>\n            </md-field>\n          </div>\n               \n\n        </div>\n      </md-card-content>\n    </md-card>\n  </form>\n</template>\n\n\n<script>\n\nimport {\n    required,   \n   \n  } from \"vuelidate/lib/validators\";\n\nexport default {\n\n  name: \"edit-profile-form\",\n  \n  props: {\n    dataBackgroundColor: {\n      type: String,\n      default: \"\",\n    },\n    tableHeaderColor: {\n      type: String,\n      default: \"\"\n    },\n  },\n\n  data() {\n    return {\n\n      processo:{\n\n      ID: \"\",\n      Instancia: \"\",\n      NomeComercialMedicamento: \"\",\n      NumRename: \"\",\n      Status: \"\",\n      Operador:\"\",\n\n      },\n\n      processos: [],\n\n      type: [\"\", \"info\", \"success\", \"warning\", \"danger\"],\n      notifications: {\n        topCenter: false\n      }\n         \n    };\n  },\n\n  validations: {\n      processo: {\n         ID: {\n           required           \n        }\n                 \n      }\n      },\n\n  \n\n methods:{\n\n   getValidationClass (fieldName) {\n        const field = this.$v.processo[fieldName]\n\n        if (field) {\n          return {\n            'md-invalid': field.$invalid && field.$dirty\n          }\n        }\n      },\n\n      async submitForm(){\n\n        this.$v.$touch();\n\n        if (!this.$v.$invalid) {\n                   \n          try {\n             \n               const response = await this.$http.get('http://localhost:8080/api/PesquisarProcessoPorNumero/'+this.processo.ID)  ; \n              //.then( res => {\n                 console.log(response.data);\n                 \n                 const retorno = response.data;\n\n                 var convert = JSON.parse(retorno.response);\n\n                  this.processo.NomeComercialMedicamento = convert.NomeComercialMedicamento;\n                  this.processo.Instancia = convert.Instancia;\n                  this.processo.NumRename = convert.NumRename;\n                  this.processo.Status = convert.Status;\n                  this.processo.Operador = convert.Operador;\n\n                  this.processos[0] = this.processo;\n\n                   //alert (retorno.NomeComercialMedicamento);\n            // }); \n             \n              \n          } catch(e) {\n              console.error(e);\n          }\n        }\n                 \n        },\n\n        \n    notifyVue(verticalAlign, horizontalAlign) {\n      var color = Math.floor(Math.random() * 4 + 1);\n      this.$notify({\n        message:\n          \"Processo Judicial salvo com sucesso na blockchain.\",\n        icon: \"add_alert\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: this.type[color]\n      });\n    },\n        \n    },\n\n\n};\n</script>\n<style></style>\n"],"sourceRoot":"src/pages/ProcessoJudicial"}]}