,EAAA;AAEA,IAAA,kBAFA,8BAEA,SAFA,EAEA;AACA,UAAA,KAAA,GAAA,KAAA,EAAA,CAAA,IAAA,CAAA,SAAA,CAAA;;AAEA,UAAA,KAAA,EAAA;AACA,eAAA;AACA,wBAAA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA;AADA,SAAA;AAGA;AACA,KAVA;AAaA,IAAA,UAbA,wBAaA;AAAA;;AAEA,WAAA,QAAA;AAEA,WAAA,EAAA,CAAA,MAAA;;AAGA,UAAA,CAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AAGA,QAAA,KAAA,CAAA,IAAA,CAAA,sCAAA,EAAA,KAAA,IAAA,EACA;AADA,SAEA,IAFA,CAEA,YAAA;AACA,UAAA,KAAA,CAAA,kBAAA,CAAA,KAAA,EAAA,QAAA;AACA,SAJA,WAKA,UAAA,KAAA,EAAA;AAEA,cAAA,GAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA,CAAA,SAAA,CAAA;AACA,cAAA,MAAA,GAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,cAAA,IAAA,GAAA,MAAA,CAAA,CAAA,CAAA,CAJA,CAMA;;AACA,cAAA,YAAA,GAAA,IAAA,CAAA,QAAA,CAAA,OAAA;;AAEA,UAAA,KAAA,CAAA,gBAAA,CAAA,KAAA,EAAA,QAAA,EAAA,YAAA;AAEA,SAhBA,aAgBA,YAAA,CACA;AACA,SAlBA;AAmBA;;AAEA,WAAA,OAAA,GAAA,KAAA;AAIA,KAhDA;AAkDA,IAAA,QAlDA,sBAkDA;AAEA;AACA,WAAA,OAAA,GAAA,IAAA,CAHA,CAKA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,KA/DA;AAiEA,IAAA,kBAjEA,8BAiEA,aAjEA,EAiEA,eAjEA,EAiEA;AAEA,WAAA,OAAA,GAAA,KAAA;AAEA,WAAA,OAAA,CAAA;AACA,QAAA,OAAA,EACA,uCAFA;AAGA,QAAA,IAAA,EAAA,WAHA;AAIA,QAAA,eAAA,EAAA,eAJA;AAKA,QAAA,aAAA,EAAA,aALA;AAMA,QAAA,IAAA,EAAA;AANA,OAAA;AAQA,KA7EA;AA+EA,IAAA,gBA/EA,4BA+EA,aA/EA,EA+EA,eA/EA,EA+EA,YA/EA,EA+EA;AAEA,WAAA,OAAA,CAAA;AACA,QAAA,OAAA,EACA,YAFA;AAGA,QAAA,IAAA,EAAA,WAHA;AAIA,QAAA,eAAA,EAAA,eAJA;AAKA,QAAA,aAAA,EAAA,aALA;AAMA,QAAA,IAAA,EAAA;AANA,OAAA;AAQA;AAzFA;AAnDA,CAAA","sourcesContent":["<template>\n\n  <form novalidate class=\"md-layout\" @submit.prevent=\"submitForm\">\n    <md-card>\n\n\n      <md-card-header :data-background-color=\"dataBackgroundColor\">\n        <h4 class=\"title\">NerdStore Vestuário</h4>\n        <p class=\"category\">Cadastro de Lote</p>\n      </md-card-header>\n\n\n      <md-card-content>\n\n         <!--<md-progress-spinner class=\"md-accent\" md-mode=\"indeterminate\"  v-show=\"showProgressBar\"> </md-progress-spinner> -->\n\n        <div class=\"md-layout\">\n\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n            \n            <md-field :class=\"getValidationClass('id')\">            \n              <label>Id do Lote (ID)</label>\n              <md-input v-model=\"form.id\" id=\"id\" type=\"text\" :disabled=\"sending\"></md-input>\n              <span class=\"md-error\" v-if=\"!$v.form.id.required\">O id do lote é obrigatório!</span>\n              <!--<span class=\"md-error\" v-else-if=\"!$v.form.id.minlength\">Invalid first name</span>-->\n            </md-field>\n\n          </div>\n\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field :class=\"getValidationClass('numeroLote')\">\n              <label>Número do Lote</label>\n              <md-input v-model=\"form.numeroLote\" id=\"numeroLote\" type=\"text\" :disabled=\"sending\"></md-input>\n              <span class=\"md-error\" v-if=\"!$v.form.numeroLote.required\">O numero do lote é obrigatório!</span>           \n            </md-field>\n          </div>\n\n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n            <md-field>\n              <label>Quantidade Fabricada</label>\n              <md-input v-model=\"form.quantidadeFabricadaLote\" type=\"text\"></md-input>\n            </md-field>\n          </div>\n\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field>\n              <label>Tipo Vestuario</label>\n              <md-input v-model=\"form.tipoVestuario\" type=\"text\"></md-input>\n            </md-field>\n          </div>\n\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field>\n              <label>Operador</label>\n              <md-input v-model=\"form.operador\" type=\"text\"></md-input>\n            </md-field>\n          </div>\n          \n          <div class=\"md-layout-item md-small-size-100 md-size-33\">\n\n          <md-field :class=\"getValidationClass('caracteristicasDesign')\">\n            \n             <label>Caracteristicas Design</label>\n             <md-input v-model=\"form.caracteristicasDesign\" id=\"caracteristicasDesign\" type=\"text\"></md-input>\n             <span class=\"md-error\" v-if=\"!$v.form.caracteristicasDesign.required\">As caracteristicas do vestuário são obrigatórias!</span>           \n          </md-field>\n\n          <div class=\"md-layout-item md-small-size-100 md-size-50\">\n            <md-field>\n              <label>Tamanho</label>\n              <md-input v-model=\"form.tamanho\" type=\"text\"></md-input>\n            </md-field>\n          </div>\n\n          </div>\n          \n                      \n          <div class=\"md-layout-item md-size-100 text-right\">\n            <md-button class=\"md-raised md-success\" type =\"submit\"\n              >Cadastrar</md-button\n            >\n          </div>\n        </div>\n        \n\n      </md-card-content>\n    </md-card>\n  </form>\n</template>\n\n<script>\n\nimport axios from 'axios';\nimport { validationMixin } from 'vuelidate';\n\n  import {\n    required,   \n    minLength\n   \n  } from \"vuelidate/lib/validators\";\n\nexport default {\n  name: \"edit-profile-form\",\n  mixins: [validationMixin],\n  props: {\n    dataBackgroundColor: {\n      type: String,\n      default: \"\",\n    },\n  },\n  data() {\n    return {\n\n      form:{\n      id: null,\n      numeroLote: null,\n      tipoVestuario: null,\n      quantidadeFabricadaLote: null,\n      tamanho: null,\n      caracteristicasDesign: null,\n      operador: null\n\n      },\n\n      type: [\"\", \"info\", \"success\", \"warning\", \"danger\"],\n      notifications: {\n        topCenter: false\n      },\n\n      sending: false,\n      showProgressBar: false\n\n      \n         \n    };\n  },\n\n  validations: {\n      form: {\n         id: {\n           required,\n           minLength: minLength(3)\n        },\n          numeroLote: {\n           required        \n        },\n        caracteristicasDesign: {\n           required        \n        }\n      }\n      },\n\n methods:{\n\ngetValidationClass (fieldName) {\n        const field = this.$v.form[fieldName]\n\n        if (field) {\n          return {\n            'md-invalid': field.$invalid && field.$dirty\n          }\n        }\n      },\n\n\n    submitForm(){\n\n      this.saveUser();\n  \n      this.$v.$touch();\n\n\n      if (!this.$v.$invalid) {\n\n    \n            axios.post('http://localhost:8083/api/CriarLote/', this.form)\n                //.then((res) => {\n                 .then(() => {\n                     this.salvoSucessoNotify('top', 'center');\n                 })\n                .catch((error) => {\n\n                   var str = JSON.stringify(error.response.data.error.responses);\n                   var parsed = JSON.parse(str);\n                   var erro = parsed[0]; \n                     \n                   //alert (JSON.stringify(erro.response.message));\n                   var mensagemErro = erro.response.message;\n\n                 this.erroSalvarNotify('top', 'center', mensagemErro);\n\n                 }).finally(() => {\n                     //Perform action in always\n                 });\n    }\n\n    this.sending = false;\n \n\n\n        },\n\n        saveUser () {\n\n        //this.showProgressBar = true;\n        this.sending = true\n\n        // Instead of this timeout, here you can call your API\n        //window.setTimeout(() => {\n         // this.lastUser = `${this.form.firstName} ${this.form.lastName}`\n          //this.userSaved = true\n          //this.sending = false\n        //  this.showProgressBar = false\n          //this.clearForm()\n       // }, 6000)\n      },\n\n    salvoSucessoNotify(verticalAlign, horizontalAlign) {\n\n      this.sending = false;\n      \n      this.$notify({\n        message:\n          \"Lote salvo com sucesso na blockchain.\",\n        icon: \"add_alert\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: \"success\"\n      });\n    },\n\n    erroSalvarNotify(verticalAlign, horizontalAlign, mensagemErro) {\n      \n      this.$notify({\n        message:\n          mensagemErro,\n        icon: \"add_alert\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: \"danger\"\n      });\n    },\n        \n    },\n\n\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .md-progress-bar {\n    position: absolute;\n    top: 0;\n    right: 0;\n    left: 0;\n  }\n</style>\n"],"sourceRoot":"src/pages/ProcessoJudicial"}]}